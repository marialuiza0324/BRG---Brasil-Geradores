			aDados2[DISPONIVEL] := nLimCred
			oSection2:PrintLine()
			aFill(aDados2,nil)
		Endif	
				
	Endif
	nDispon += (nValor+nLimCred)
	
	
	dbSelectArea("SA6")
	// Se emitido por empresa e o SA6 for Exclusivo, posiciono na proxima conta diferenciada
