oSection1:Finish()

If (Empty(MV_PAR20))
	aFill(aDados3,nil)
	oSection3:Init()
	oSection4:Init()

	If !oReport:lEmptyLineExcel
		oSection4:Cell("E2_VENCREA"	):Disable()
