		oSection4:Cell("E2_VENCREA"	):Disable()
		oSection4:Cell("E2_PREFIXO"	):Disable()
		oSection4:Cell("TITULO"):Disable()
		oSection4:Cell("E2_TIPO"):Disable()
		oSection4:Cell("E2_EMISSAO"):Disable()
